      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import os\n",
        "\n",
        "# Load the prepared data\n",
        "prepared_data_path = 'C:/Users/ADMIN/ET6-CDSP-group-17-repo/2_data_preparation/ASOS_GraphReturns/prepared_asos_data.csv'\n",
        "try:\n",
        "    df = pd.read_csv(prepared_data_path)\n",
        "    print(f\"Successfully loaded prepared data from {prepared_data_path}\")\n",
        "except FileNotFoundError:\n",
        "    print(f\"Error: {prepared_data_path} not found. Please ensure the data preparation step was completed.\")\n",
        "    exit()\n",
        "\n",
        "print(\"\n--- Initial Data Overview ---\")\n",
        "print(\"Shape:\", df.shape)\n",
        "print(\"Columns:\", df.columns.tolist())\n",
        "print(\"Data Types:\n", df.info())\n",
        "print(\"Missing Values:\n", df.isnull().sum()[df.isnull().sum() > 0])\n",
        "print(\"First 5 rows:\n", df.head())\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# --- Exploratory Data Analysis (EDA) ---\n",
        "\n",
        "# 1. Distribution of the target variable (isReturned)\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.countplot(x='isReturned', data=df)\n",
        "plt.title('Distribution of Product Returns')\n",
        "plt.xlabel('Is Returned (0=No, 1=Yes)')\n",
        "plt.ylabel('Count')\n",
        "plt.show()\n",
        "\n",
        "print(\"\nValue Counts for isReturned:\n", df['isReturned'].value_counts())\n",
        "print(\"Percentage of Returns:\n", df['isReturned'].value_counts(normalize=True) * 100)\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 2. Return Rate by Gender (isMale)\n",
        "if 'isMale' in df.columns:\n",
        "    plt.figure(figsize=(7, 5))\n",
        "    sns.barplot(x='isMale', y='isReturned', data=df)\n",
        "    plt.title('Return Rate by Gender (0=Female, 1=Male)')\n",
        "    plt.xlabel('Gender')\n",
        "    plt.ylabel('Average Return Rate')\n",
        "    plt.show()\n",
        "else:\n",
        "    print(\"'isMale\' column not found for gender analysis.\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 3. Return Rate by Shipping Country (Top 10)\n",
        "# Assuming one-hot encoded country columns are present, we need to reconstruct or use the original if available.\n",
        "# If 'shippingCountry' is still a column (not one-hot encoded), use it directly.\n",
        "if 'shippingCountry' in df.columns:\n",
        "    top_countries = df['shippingCountry'].value_counts().nlargest(10).index\n",
        "    plt.figure(figsize=(10, 6))\n",
        "    sns.barplot(x='shippingCountry', y='isReturned', data=df[df['shippingCountry'].isin(top_countries)])\n",
        "    plt.title('Return Rate by Top 10 Shipping Countries')\n",
        "    plt.xlabel('Shipping Country')\n",
        "    plt.ylabel('Average Return Rate')\n",
        "    plt.xticks(rotation=45, ha='right')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "else:\n",
        "    print(\"'shippingCountry' column not found for country analysis. Assuming one-hot encoded columns are used.\")\n",
        "    # If shippingCountry is one-hot encoded, we can analyze the Country_X columns\n",
        "    country_cols = [col for col in df.columns if col.startswith('Country_') and col != 'Country_Other']\n",
        "    if country_cols:\n",
        "        country_return_rates = {col: df[df[col] == 1]['isReturned'].mean() for col in country_cols}\n",
        "        country_return_rates_series = pd.Series(country_return_rates).sort_values(ascending=False).head(10)\n",
        "        plt.figure(figsize=(10, 6))\n",
        "        sns.barplot(x=country_return_rates_series.index, y=country_return_rates_series.values)\n",
        "        plt.title('Return Rate by Top 10 One-Hot Encoded Countries')\n",
        "        plt.xlabel('Country')\n",
        "        plt.ylabel('Average Return Rate')\n",
        "        plt.xticks(rotation=45, ha='right')\n",
        "        plt.tight_layout()\n",
        "        plt.show()\n",
        "    else:\n",
        "        print(\"No 'Country_X' columns found for country analysis.\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 4. Return Rate by Product Type (Top 10)\n",
        "if 'productType' in df.columns:\n",
        "    top_product_types = df['productType'].value_counts().nlargest(10).index\n",
        "    plt.figure(figsize=(10, 6))\n",
        "    sns.barplot(x='productType', y='isReturned', data=df[df['productType'].isin(top_product_types)])\n",
        "    plt.title('Return Rate by Top 10 Product Types')\n",
        "    plt.xlabel('Product Type')\n",
        "    plt.ylabel('Average Return Rate')\n",
        "    plt.xticks(rotation=45, ha='right')\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "else:\n",
        "    print(\"'productType' column not found for product type analysis. Assuming one-hot encoded columns are used.\")\n",
        "    # If productType is one-hot encoded, we can analyze the productType_X columns\n",
        "    product_type_cols = [col for col in df.columns if col.startswith('productType_') and col != 'productType_Other']\n",
        "    if product_type_cols:\n",
        "        product_type_return_rates = {col: df[df[col] == 1]['isReturned'].mean() for col in product_type_cols}\n",
        "        product_type_return_rates_series = pd.Series(product_type_return_rates).sort_values(ascending=False).head(10)\n",
        "        plt.figure(figsize=(10, 6))\n",
        "        sns.barplot(x=product_type_return_rates_series.index, y=product_type_return_rates_series.values)\n",
        "        plt.title('Return Rate by Top 10 One-Hot Encoded Product Types')\n",
        "        plt.xlabel('Product Type')\n",
        "        plt.ylabel('Average Return Rate')\n",
        "        plt.xticks(rotation=45, ha='right')\n",
        "        plt.tight_layout()\n",
        "        plt.show()\n",
        "    else:\n",
        "        print(\"No 'productType_X' columns found for product type analysis.\")\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 5. Correlation Matrix (Top features with isReturned)\n",
        "# Select numerical columns and the target variable\n",
        "numerical_cols = df.select_dtypes(include=['number']).columns.tolist()\n",
        "if 'isReturned' in numerical_cols:\n",
        "    numerical_cols.remove('isReturned')\n",
        "\n",
        "# Calculate correlations with 'isReturned'\n",
        "correlations = df[numerical_cols + ['isReturned']].corr()['isReturned'].sort_values(ascending=False)\n",
        "\n",
        "# Plot top 15 positive and negative correlations\n",
        "top_correlations = pd.concat([correlations.head(15), correlations.tail(15)])\n",
        "\n",
        "plt.figure(figsize=(10, 8))\n",
        "sns.barplot(x=top_correlations.values, y=top_correlations.index, palette='viridis')\n",
        "plt.title('Top 15 Features Correlated with Product Returns')\n",
        "plt.xlabel('Correlation Coefficient')\n",
        "plt.ylabel('Feature')\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ],
